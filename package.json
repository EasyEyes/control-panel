{
  "name": "threshold-scientist",
  "private": "true",
  "version": "1.0.0",
  "description": "Experimenter-facing web-app, used to generate the necessary files for an online experiment from a single CSV file, all from and within the browser.",
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss,ts}\"",
    "prepare": "husky install",
    "update:git": "git add -A && git commit -m 'update' && git push",
    "files": "node server/writeAllFilePaths.js && prettier source/components/files.ts --write",
    "start": "webpack serve --env=development",
    "build": "webpack --env=production",
    "clean": "rm -r node_modules",
    "build:threshold": "cd threshold && npm run netlify:website",
    "prepare:commit": "npm run files",
    "netlify:install": "npm ci",
    "netlify": "run-s netlify:install build build:threshold"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "dataframe-js": "^1.4.4",
    "file-saver": "^2.0.5",
    "firebase": "^9.15.0",
    "jszip": "^3.10.1",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-table": "^7.8.0",
    "sweetalert2": "^11.6.16"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/bootstrap": "^5.2.6",
    "@types/dropzone": "^5.7.4",
    "@types/file-saver": "^2.0.5",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "express": "^4.18.2",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-dev-server": "^4.11.1",
    "xlsx": "^0.18.5"
  },
  "lint-staged": {
    "*.{js,json,md,html,css,scss,ts}": "prettier --write"
  }
}
