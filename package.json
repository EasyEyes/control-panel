{
  "name": "threshold-scientist",
  "private": "true",
  "version": "1.0.0",
  "description": "Experimenter-facing web-app, used to generate the necessary files for an online experiment from a single CSV file, all from and within the browser.",
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss,ts}\"",
    "prepare": "husky install",
    "update:git": "git add -A && git commit -m 'update' && git push",
    "files": "node server/writeAllFilePaths.js && prettier src/files.ts --write && git add src/files.ts",
    "start": "webpack --env=development",
    "build": "webpack --env=production",
    "prepare:commit": "npm run files",
    "netlify": "npm ci && npm run build && rm -r node_modules && cd threshold && npm run netlify:website"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/dropzone": "^5.7.4",
    "@types/papaparse": "^5.3.2",
    "@types/webfontloader": "^1.6.34",
    "dataframe-js": "^1.4.4",
    "dropzone": "^6.0.0-beta.2",
    "jszip": "^3.7.1",
    "papaparse": "^5.3.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.9",
    "bootstrap": "^5.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.6.0",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "xlsx": "^0.18.2"
  },
  "lint-staged": {
    "*.{js,json,md,html,css,scss,ts}": "prettier --write"
  }
}
