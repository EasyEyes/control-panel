{
  "name": "threshold-scientist",
  "private": "true",
  "version": "1.0.0",
  "description": "Experimenter-facing web-app, used to generate the necessary files for an online experiment from a single CSV file, all from and within the browser.",
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss,ts}\"",
    "prepare": "husky install",
    "update:git": "git add -A && git commit -m 'update' && git push",
    "files": "node server/writeAllFilePaths.js && prettier source/components/files.ts --write",
    "start": "webpack serve --env=development",
    "build": "webpack --env=production",
    "clean": "rm -r node_modules",
    "build:threshold": "cd threshold && npm run netlify:website",
    "prepare:commit": "npm run files",
    "netlify:install": "npm ci",
    "netlify": "run-s netlify:install build build:threshold"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "dataframe-js": "^1.4.4",
    "file-saver": "^2.0.5",
    "firebase": "^10.6.0",
    "jszip": "^3.10.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-table": "^7.8.0",
    "sweetalert2": "^11.6.13"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@svgr/webpack": "^8.1.0",
    "@types/bootstrap": "^5.2.9",
    "@types/dropzone": "^5.7.7",
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/regenerator-runtime": "^0.13.4",
    "@types/webfontloader": "^1.6.37",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "log-here-now": "^0.3.7",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "lint-staged": {
    "*.{js,json,md,html,css,scss,ts}": "prettier --write"
  }
}
