{
  "name": "control-panel",
  "private": "true",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,html,css,scss,ts}\"",
    "prepare": "husky install",
    "files": "node server/writeAllFilePaths.js && prettier components/files.js --write && git add components/files.js",
    "update:git": "git add -A && git commit -m 'update' && git push",
    "glossary": "node server/prepareGlossary.js && git add components/glossary.js",
    "start": "webpack --env=development",
    "build": "webpack --env=production",
    "netlify": "npm i && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/dropzone": "^5.7.4",
    "@types/papaparse": "^5.3.1",
    "dataframe-js": "^1.4.4",
    "dropzone": "^6.0.0-beta.1",
    "express": "^4.17.1",
    "jszip": "^3.7.1",
    "papaparse": "^5.3.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "googleapis": "^88.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "xlsx": "^0.17.3"
  },
  "lint-staged": {
    "*.{js,json,md,html,css,scss,ts}": "prettier --write"
  }
